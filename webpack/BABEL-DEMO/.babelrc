{
  "presets": [
    [
      "@babel/preset-env", //preset可以以作为babel的集合, 一堆plugin，es6,7,8...
      {
        "useBuiltIns": "usage", //按需引入
        "corejs": 3
      }
    ]
  ],
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "absoluteRuntime": false,
        "corejs": 3,
        "helpers": true,
        "regenerator": true,
        "useESModules": false
      }
    ]
  ]
}